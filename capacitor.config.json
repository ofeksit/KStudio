{
  "appId": "io.kstudio.os",
  "appName": "KStudio",
  "webDir": "www",
  "plugins": {
    "StatusBar": {
      "backgroundColor": "#000000",
      "style": "LIGHT"
    }
  },
  "cordova": {
    "preferences": {
      "ScrollEnabled": "false",
      "BackupWebStorage": "none",
      "SplashMaintainAspectRatio": "true",
      "FadeSplashScreenDuration": "300",
      "SplashShowOnlyFirstTime": "false",
      "SplashScreen": "screen",
      "SplashScreenDelay": "3000"
    }
  },
  "android": {
    "buildType": "debug"
  }
}
